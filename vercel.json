{
    "version": 2,
    "builds": [
      {
        "src": "dist/myprofile/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://backendprofileargprog-production.up.railway.app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/myprofile/$1"
      }
    ]
  }
  